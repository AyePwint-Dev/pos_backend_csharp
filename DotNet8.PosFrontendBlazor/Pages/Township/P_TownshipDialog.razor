<MudDialog>
    <DialogContent>
        <MudSelect T="string" Label="State Code" AnchorOrigin="Origin.BottomCenter" @bind-Value="@reqModel.StateCode">
            @foreach (var item in lstStateCode.Data.Township)
            {
                <MudSelectItem Value="@item.TownshipCode">@item.TownshipName</MudSelectItem>
            }
        </MudSelect>
        <MudTextField @bind-Value="@reqModel.TownshipName" Label="Township Name" />
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel">Cancel</MudButton>
        <MudButton Color="Color.Success" OnClick="SaveAsync">Save</MudButton>
    </DialogActions>
</MudDialog>