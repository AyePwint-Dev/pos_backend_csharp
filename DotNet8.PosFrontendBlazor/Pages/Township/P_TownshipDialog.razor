<MudDialog>
    <TitleContent>
        <MudText Typo="Typo.h6">
            @title
        </MudText>
    </TitleContent>
    <DialogContent>
        <MudTextField @bind-Value="@reqModel.StateCode" Label="State Code" />
        <MudTextField @bind-Value="@reqModel.TownshipName" Label="Township Name" />
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel">Cancel</MudButton>
        <MudButton Color="Color.Success" OnClick="SaveAsync">Save</MudButton>
    </DialogActions>
</MudDialog>
